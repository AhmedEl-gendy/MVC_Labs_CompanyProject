
@{
    ViewData["Title"] = "LogIn";
}
@model Employee
<h1>LogIn</h1>

<form action="/Employee/LoginSSN">
    <label for="SSN">Username</label>
    <input type="text" id="SSN" name="SSN" />
    <label for="Fname">Name</label>
    <input type="text" id="Fname" name="Fname" />
    <input type="submit" value="Log in"/>
</form>